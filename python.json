{
    "problemMatcher": [
        {
            "owner": "pythonx",
            "pattern": [
                {
                    "regexp": "^.*File \"([^\"]*).py\", line (\\d+).*",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "^.*raise.*$"
                },
                {
                    "regexp": "^\\s*(.*)\\s*$",
                    "message": 1
                }
            ]
        }
    ]
}