{
    "problemMatcher": [
        {
            "owner": "python",
            "pattern": [
                {
                    "regexp": "^.*File \"(/home/runner/work/[^\"]*).py\", line (\\d+).*",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "^.*raise.*$"
                },
                {
                    "regexp": "^\\s*(.*)\\s*$",
                    "message": 1
                }
            ]
        }
    ]
}